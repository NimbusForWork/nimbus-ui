{
  "name": "@nimbusforwork/nimbus-ui",
  "version": "1.0.22",
  "description": "Nimbus UI Components",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "jsnext:main": "build/index.es.js",
  "scripts": {
    "build": "rollup -c",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./static",
    "storybook:build": "build-storybook -c .storybook -o storybook -s ./static",
    "lint": "eslint '**/*.{ts,tsx}'",
    "test": "jest -u",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "deploy": "npm version patch; npm publish --access=public"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.7.2",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-docs": "^5.2.6",
    "@storybook/addon-info": "^5.2.6",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-notes": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@storybook/source-loader": "^5.2.6",
    "@types/jest": "^24.0.21",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-native-vector-icons": "^6.4.4",
    "@types/react-test-renderer": "^16.9.1",
    "@types/storybook__react": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.0.2",
    "date-fns": "^2.8.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.2.0",
    "husky": "^3.0.3",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.3",
    "lint-staged": "^9.2.1",
    "polished": "^3.4.2",
    "prettier": "^1.18.2",
    "react": "^16.11.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "^16.11.0",
    "react-native": "^0.61.5",
    "react-native-vector-icons": "^6.6.0",
    "react-native-web": "^0.11.7",
    "react-scripts-ts": "^3.1.0",
    "react-test-renderer": "^16.11.0",
    "rollup": "^1.26.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "rollup-plugin-url": "^3.0.0",
    "styled-components": "^4.4.1",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-native-web": "^0.11.7",
    "styled-components": "^4.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NimbusForWork/nimbus-ui.git"
  },
  "keywords": [
    "Nimbus",
    "react",
    "react-native"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NimbusForWork/nimbus-ui/issues"
  },
  "homepage": "https://github.com/NimbusForWork/nimbus-ui#readme",
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.(ttf|woff|woff2)$": "<rootDir>/__mocks__/fileMock.js",
      "styled-components": "<rootDir>/node_modules/styled-components/native/dist/styled-components.native.cjs.js"
    }
  },
  "files": [
    "build"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
